				function openWinPop(url,name){
					var nWinLeft = (screen.width-950)/2;
					var nWinTop = (screen.height-620)/2;
					var win_style = "menubar=no,toolbar=no,location=no,directories=no,status=yes,resizable=yes,scrollbars=no,width=940,height=603,top=" + nWinTop + ",left=" + nWinLeft;
